{
  "name": "express-react-boilerplate",
  "version": "0.0.0",
  "description": "",
  "private": true,
  "author": "Jovani Warguez",
  "main": "bin/www",
  "repository": {
    "type": "git",
    "url": "https://github.com/jovanidash21/express-react-boilerplate.git"
  },
  "scripts": {
    "start": "node ./bin/www",
    "build": "concurrently \"npm run build:client\" \"npm run build:server\"",
    "build:client": "webpack -p",
    "build:server": "cross-env NODE_ENV=production node ./bin/www",
    "dev": "concurrently -k \"webpack --watch\" \"nodemon ./bin/www --watch build\""
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "concurrently": "^3.5.0",
    "cookie-parser": "^1.4.3",
    "debug": "^2.6.8",
    "express": "^4.15.3",
    "morgan": "^1.8.2",
    "pug": "^2.0.0-rc.3",
    "serve-favicon": "^2.4.3"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.18.13",
    "cross-env": "^5.0.3",
    "css-loader": "^0.28.4",
    "eslint": "^4.3.0",
    "eslint-plugin-react": "^7.1.0",
    "nodemon": "^1.11.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.4.1"
  }
}
